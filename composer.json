{
    "name": "docler-labs/api-client-base",
    "version": "3.0.1",
    "description": "SDK Generator - API Client Base",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "rest",
        "api",
        "api-client",
        "openapi"
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.2",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "DoclerLabs\\ApiClientBase\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DoclerLabs\\ApiClientBase\\Test\\Unit\\": "test/suite/unit/"
        }
    }
}
